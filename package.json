{
  "name": "kombu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "bundlewasm": "node scripts/bundlewasm.js",
    "check": "tsc --noEmit",
    "format": "prettier --write --no-semi .",
    "preview": "vite preview",
    "test": "vitest",
    "asbuild:release": "asc assembly/index.ts --target release --runtime stub -O0 --noExportMemory",
    "asbuild": "npm run asbuild:release && npm run bundlewasm",
    "start": "npx serve ."
  },
  "devDependencies": {
    "assemblyscript": "^0.27.11",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@wasmgroundup/emit": "^0.2.11"
  }
}
